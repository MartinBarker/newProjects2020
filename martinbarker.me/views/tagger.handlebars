<!-- page main content -->
<div class="container-fluid" style='color: {{textColor5}}'>

    <div style="margin-top:-30px;">

        <!-- Header Text -->

        <h5 class="widgettitle">Use a Discogs URL or local files to generate a timestamped tracklist and metadata tags.</h5>
        <hr>

        <!-- Discogs Input Form -->
        <div class="form-group">
            <label>
                <h5>Discogs URL:</h5>
            </label>
            <input size="12" type="text" placeholder='Discogs URL' name="url" id="urlInput">
            <button style="cursor: pointer;" id='urlInputButton' name="data" type="button" onclick="inputOption1(document.getElementById('urlInput').value)">Submit</button>
        
        </div>


        <!-- File Input Selection -->
        <label>
            <h5>Files: </h5>
        </label> <input style="cursor: pointer;" type="file" id="file" multiple="multiple" />
        <audio id="audio"></audio>

        <hr>
        <h3>Timestamped Tracklist:</h3>

        <!-- tagger output -->
        <div class='outputTaggerBox' style='margin-left:11px;'>
            <div class='row'>
                <textarea  class="inputbox" id="inputBox" rows="7" cols="44" placeholder="Tracklist generated by http://tagger.site:
00:00 - 09:29 01 - wedding song
09:29 - 13:31 02 - she
13:31 - 19:14 03 - long song
19:14 - 20:30 04 - sea gull
20:30 - 24:42 05 - for priscilla
24:42 - 34:14 06 - the delta song
34:14 - 39:35 07 - why
"></textarea>
                <button class="btn" style="cursor: pointer;" id='copyToClipboarad' onclick="copyToClipboard('#inputBox')" type="button">Copy</button>
            </div>
        </div>

        <hr>
        <h3>Comma-Separated Metadata Tags:</h3>

        <table align="center" id="adjustments">
                        <tbody>
                            <!-- Columns -->
                            <tr>
                                <th>Include</th>
                                <th>Tags Type</th>
                                <th>Depth</th>
                                <th># of Tags</th>
                            </tr>

                            <!-- Artists -->
                            <tr>
                                <!-- Checkbox -->
                                <td>
                                    <input type="checkbox" class="releaseArtistsCheckbox" id="releaseArtistsCheckbox"
                                        checked="" onchange="prepUpdateTagsBox()">
                                </td>
                                <!-- Description -->
                                <td>Release Artist(s)</td>
                                <!-- Depth -->
                                <td>
                                    <input type="range" class="releaseArtistsSlider" id="releaseArtistsSlider" min="0"
                                        max="100" value="100" onchange="prepUpdateTagsBox()">
                                    <div id="releaseArtistsSliderValue">100%</div>
                                </td>
                                <!-- # of Tags -->
                                <td id='releaseArtistsNumber'>0</td>
                            </tr>

                            <!-- Release Info -->
                            <tr>
                                <td>
                                    <input type="checkbox" class="releaseInfoCheckbox" id="releaseInfoCheckbox"
                                        checked="" onchange="prepUpdateTagsBox()">
                                </td>
                                <td>Release Info</td>
                                <td>
                                    <input type="range" class="releaseInfoSlider" id="releaseInfoSlider" min="0"
                                        max="100" value="100" onchange="prepUpdateTagsBox()">
                                    <div id="releaseInfoSliderValue">100%</div>
                                </td>
                                <!-- # of Tags -->
                                <td id='releaseInfoNumber'>0</td>
                            </tr>

                            <!-- Tracklist -->
                            <tr>
                                <td>
                                    <input type="checkbox" class="tracklistCheckbox" id="tracklistCheckbox" checked=""
                                        onchange="prepUpdateTagsBox()">
                                </td>
                                <td>Tracklist</td>
                                <td>
                                    <input type="range" class="tracklistSlider" id="tracklistSlider" min="0" max="100"
                                        value="100" onchange="prepUpdateTagsBox()">
                                    <div id="tracklistSliderValue">100%</div>
                                </td>
                                <!-- # of Tags -->
                                <td id='tracklistNumber'>0</td>
                            </tr>

                            <!-- Combinations -->
                            <tr>
                                <td>
                                    <input type="checkbox" class="combinationsCheckbox" id="combinationsCheckbox"
                                        checked="" onchange="prepUpdateTagsBox()">
                                </td>
                                <td>Combinations</td>
                                <td>
                                    <input type="range" class="combinationsSlider" id="combinationsSlider" min="0"
                                        max="100" value="100" onchange="prepUpdateTagsBox()">
                                    <div id="combinationsSliderValue">100%</div>
                                </td>
                                <!-- # of Tags -->
                                <td id='combinationsNumber'>0</td>
                            </tr>

                        </tbody>
                    </table>

        <style>
            table, th, td {
                border: 1px solid black;
            }
